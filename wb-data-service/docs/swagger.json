    "definitions": {
        "core.PriceHistory": {
            "type": "object",
            "properties": {
                "dt": {
                    "type": "string",
                    "example": "2024-02-11 18:57:11.811169+00"
                },
                "id": {
                    "type": "integer",
                    "example": 1
                },
                "nm_id": {
                    "type": "integer",
                    "example": 139760729
                },
                "price": {
                    "type": "integer",
                    "example": 20199000
                }
            }
        },
        "core.Product": {
            "type": "object",
            "properties": {
                "brand": {
                    "type": "string",
                    "example": "Apple"
                },
                "brand_id": {
                    "type": "integer",
                    "example": 6049
                },
                "colors": {
                    "type": "string",
                    "example": "фиолетовый"
                },
                "created_at": {
                    "type": "string",
                    "example": "2024-02-11 18:57:11.811169+00"
                },
                "feedbacks": {
                    "type": "integer",
                    "example": 31
                },
                "name": {
                    "type": "string",
                    "example": "iPhone 14 Pro Max 1TB"
                },
                "nm_id": {
                    "type": "integer",
                    "example": 139760729
                },
                "price": {
                    "type": "integer",
                    "example": 20199000
                },
                "quantity": {
                    "type": "integer",
                    "example": 0
                },
                "rating": {
                    "type": "number",
                    "example": 5
                },
                "sale": {
                    "type": "integer",
                    "example": 6
                },
                "sale_price": {
                    "type": "integer",
                    "example": 18987000
                },
                "site_brand_id": {
                    "type": "integer",
                    "example": 16049
                },
                "supplier_id": {
                    "type": "integer",
                    "example": 887491
                },
                "updated_at": {
                    "type": "string",
                    "example": "2024-02-11 18:57:11.811169+00"
                }
            }
        },
        "core.Token": {
            "type": "object",
            "properties": {
                "access_token": {
                    "type": "string"
                },
                "refresh_token": {
                    "type": "string"
                }
            }
        },
